{
  "$id": "http://example.com/example.json",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "password": {
      "$id": "/properties/password",
      "type": "string",
      "title": "The Password Schema ",
      "default": "",
      "minLength": 5,
      "maxLength": 55,
      "pattern": "^[a-zA-Z 0-9\\_\\-]*$",
      "examples": [
        "foo123"
      ]
    },
    "first_name": {
      "$id": "/properties/first_name",
      "type": "string",
      "title": "The First_name Schema ",
      "default": "",
      "minLength": 1,
      "maxLength": 55,
      "pattern": "^[a-zA-Z 0-9\\_\\-]*$",
      "message": {
        "pattern": "Name can only contain certian characters."
      },
      "examples": [
        "Michael"
      ]
    },
    "last_name": {
      "$id": "/properties/last_name",
      "type": "string",
      "title": "The Last_name Schema ",
      "default": "",
      "minLength": 1,
      "maxLength": 55,
      "pattern": "^[a-zA-Z 0-9\\_\\-]*$",
      "examples": [
        "Hueter"
      ]
    },
    "current_company": {
      "$id": "/properties/current_company",
      "type": "string",
      "title": "The Current_company Schema ",
      "default": "",
      "examples": [
        "rithm"
      ]
    },
    "email": {
      "$id": "/properties/email",
      "type": "string",
      "minLength": 6,
      "maxLength": 60,
      "title": "The Email Schema ",
      "default": "",
      "pattern": "^[a-zA-Z 0-9\\_\\-\\@\\+\\.]*$",
      "examples": [
        "michael@rithmschool.com"
      ]
    },
    "_token": {
      "$id": "/properties/token",
      "type": "string",
      "title": "The Token Schema ",
      "default": "",
      "examples": [
        "foo123"
      ]
    }
  }
}